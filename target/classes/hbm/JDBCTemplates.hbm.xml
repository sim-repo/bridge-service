<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.domain.log">
						
	<class name="LogJDBCTemplate" table="[jdbc templates]">					
		
		<id name="id" column="[id]">
			<generator class="native" />
		</id>	
		
		<property name="endpointId" column="[endpoint_id]"/>
		<property name="strJdbcTemplate" column="[jdbc_template_bean_id]"/>		
													
	</class>
</hibernate-mapping>
